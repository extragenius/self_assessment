<h1>Delete Questionnaire</h1>
<h2><%= @questionnaire.title %></h2>
<p>Are you sure you want to delete this questionnaire?</p>

<%= form_for(@questionnaire, :url => questionnaire_path(@questionnaire), :method => :delete) do |form| %>
<%= form.submit('Yes') %>
<%= link_to('Cancel', url_for(:back)) %>
<% end %>

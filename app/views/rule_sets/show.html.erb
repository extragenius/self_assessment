<div class="rule_set">

  <%= content_tag('div', @rule_set.description, :class => :description) if @rule_set.description.present? %>

  <table class="answers">
    <tr>
      <th>We asked:</th>
      <th>You answered:</th>
    </tr>
<% @rule_set.answers.each do |answer| %>
    <tr>
      <td><%= answer.question.title %></td>
      <td><%= answer.value %></td>
    </tr>
<% end %>
  </table>

  <p><%= link_to('Click here', @rule_set.url) %> to find suppliers who can provide relevant products and services</p>

</div>
